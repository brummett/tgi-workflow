<workflow name="contig predict genes" parallelBy="fasta file">
  <link fromOperation="input connector" fromProperty="fasta file" toOperation="glimmer3" toProperty="fasta file" />
  <link fromOperation="input connector" fromProperty="fasta file" toOperation="genemark" toProperty="fasta file" />
  <link fromOperation="input connector" fromProperty="fasta file" toOperation="trnascan" toProperty="fasta file" />
  <link fromOperation="input connector" fromProperty="fasta file" toOperation="rnamer" toProperty="fasta file" />
  <link fromOperation="input connector" fromProperty="fasta file" toOperation="rfamscan" toProperty="fasta file" />
  <link fromOperation="input connector" fromProperty="glimmer3 model" toOperation="glimmer3" toProperty="model file" />
  <link fromOperation="input connector" fromProperty="glimmer3 pwm" toOperation="glimmer3" toProperty="pwm file" />
  <link fromOperation="input connector" fromProperty="gc content" toOperation="genemark" toProperty="gc content" />
  <link fromOperation="glimmer3" fromProperty="bio seq feature" toOperation="cat seq feature" toProperty="glimmer3 feature" />
  <link fromOperation="genemark" fromProperty="bio seq feature" toOperation="cat seq feature" toProperty="genemark feature" />
  <link fromOperation="trnascan" fromProperty="bio seq feature" toOperation="cat seq feature" toProperty="trnascan feature" />
  <link fromOperation="rnamer" fromProperty="bio seq feature" toOperation="cat seq feature" toProperty="rnamer feature" />
  <link fromOperation="rfamscan" fromProperty="bio seq feature" toOperation="cat seq feature" toProperty="rfamscan feature" />
  <link fromOperation="cat seq feature" fromProperty="all features" toOperation="output connector" toProperty="bio seq feature" />
  <link fromOperation="cat seq feature" fromProperty="result" toOperation="output connector" toProperty="result" />
  <operation name="glimmer3">
    <operationtype typeClass="Workflow::OperationType::Dummy">
      <inputproperty>fasta file</inputproperty>
      <inputproperty>model file</inputproperty>
      <inputproperty>pwm file</inputproperty>
      <outputproperty>bio seq feature</outputproperty>
      <outputproperty>result</outputproperty>
    </operationtype>
  </operation>
  <operation name="genemark">
    <operationtype typeClass="Workflow::OperationType::Dummy">
      <inputproperty>fasta file</inputproperty>
      <inputproperty>gc content</inputproperty>
      <outputproperty>bio seq feature</outputproperty>
      <outputproperty>result</outputproperty>
    </operationtype>
  </operation>
  <operation name="trnascan">
    <operationtype typeClass="Workflow::OperationType::Dummy">
      <inputproperty>fasta file</inputproperty>
      <outputproperty>bio seq feature</outputproperty>
      <outputproperty>result</outputproperty>
    </operationtype>
  </operation>
  <operation name="rnamer">
    <operationtype typeClass="Workflow::OperationType::Dummy">
      <inputproperty>fasta file</inputproperty>
      <outputproperty>bio seq feature</outputproperty>
      <outputproperty>result</outputproperty>
    </operationtype>
  </operation>
  <operation name="rfamscan">
    <operationtype typeClass="Workflow::OperationType::Dummy">
      <inputproperty>fasta file</inputproperty>
      <outputproperty>bio seq feature</outputproperty>
      <outputproperty>result</outputproperty>
    </operationtype>
  </operation>
  <operation name="cat seq feature">
    <operationtype typeClass="Workflow::OperationType::Dummy">
      <inputproperty>glimmer3 feature</inputproperty>
      <inputproperty>genemark feature</inputproperty>
      <inputproperty>trnascan feature</inputproperty>
      <inputproperty>rnamer feature</inputproperty>
      <inputproperty>rfamscan feature</inputproperty>
      <outputproperty>all features</outputproperty>
      <outputproperty>result</outputproperty>
    </operationtype>
  </operation>
  <operationtype typeClass="Workflow::OperationType::Model">
    <inputproperty>fasta file</inputproperty>
    <inputproperty>glimmer3 model</inputproperty>
    <inputproperty>glimmer3 pwm</inputproperty>
    <inputproperty>gc content</inputproperty>
    <outputproperty>bio seq feature</outputproperty>
    <outputproperty>result</outputproperty>
  </operationtype>
</workflow>
