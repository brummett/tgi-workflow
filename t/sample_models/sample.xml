<?xml version='1.0' standalone='yes'?>
<workflow name="Example Workflow">
  <link fromOperation="wait for sleep and echo" fromProperty="echo result" toOperation="echo" toProperty="result" />
  <link fromOperation="output connector" fromProperty="result" toOperation="wait for sleep and echo" toProperty="echo result" />
  <link fromOperation="echo" fromProperty="input" toOperation="input connector" toProperty="model input string" />
  <link fromOperation="output connector" fromProperty="today" toOperation="time" toProperty="today" />
  <link fromOperation="output connector" fromProperty="model output string" toOperation="echo" toProperty="output" />
  <link fromOperation="sleep" fromProperty="seconds" toOperation="input connector" toProperty="sleep time" />
  <link fromOperation="wait for sleep and echo" fromProperty="sleep result" toOperation="sleep" toProperty="result" />
  <operation name="echo">
    <operationtype commandClass="Workflow::Test::Command::Echo" typeClass="Workflow::OperationType::Command" />
  </operation>
  <operation name="time">
    <operationtype commandClass="Workflow::Test::Command::Time" typeClass="Workflow::OperationType::Command" />
  </operation>
  <operation name="wait for sleep and echo">
    <operationtype typeClass="Workflow::OperationType::Block">
      <property>echo result</property>
      <property>sleep result</property>
    </operationtype>
  </operation>
  <operation name="sleep">
    <operationtype commandClass="Workflow::Test::Command::Sleep" typeClass="Workflow::OperationType::Command" />
  </operation>
  <operationtype typeClass="Workflow::OperationType::Model">
    <inputproperty>model input string</inputproperty>
    <inputproperty>sleep time</inputproperty>
    <outputproperty>model output string</outputproperty>
    <outputproperty>result</outputproperty>
    <outputproperty>today</outputproperty>
  </operationtype>
</workflow>
